        pytesseract.pytesseract.tesseract_cmd = r"C:\Users\thiag\AppData\Local\Tesseract-OCR\Tesseract.exe"
        resultado = pytesseract.image_to_string(imagem)
        valor = resultado.replace('R$', '')
        valor2 = valor.replace(',', '')
        dig = len(valor2.strip())
        if math.trunc(int(valor2[:dig - 2]) / 4096) < 1:
            mult = 1
        elif math.trunc(int(valor2[:dig - 2]) / 4096) < mult0:
            mult = mult0
        else:
            mult = math.trunc(int(valor2[:dig - 2]) / 4096)

        if locateOnScreen('26-2.PNG', confidence=0.95, region=(983, 97, 35, 20)):
            num1 = 26
        elif locateOnScreen('34-2.PNG', confidence=0.95, region=(983, 97, 35, 20)):
            num1 = 34
        elif locateOnScreen('18-2.PNG', confidence=0.95, region=(983, 97, 35, 20)):
