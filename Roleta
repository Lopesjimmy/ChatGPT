# ============================ Identificação do Número Sorteado =======================================================

while True:
    if locateOnScreen('Sessao_expirada.PNG', confidence=0.5, region=(852, 300, 210, 60)):
        pyautogui.press('F5')
        sleep(1)
        jogar = locateCenterOnScreen('Jogar.PNG', confidence=0.5, region=(932, 487, 104, 45))
        sleep(1)
        pyautogui.moveTo(jogar)
        sleep(1)
        pyautogui.click()
        sleep(1)
        maximizar = locateCenterOnScreen('Maximizar.PNG', confidence=0.5)
        sleep(1)
        pyautogui.moveTo(maximizar)
        sleep(1)
        pyautogui.click()
        sleep(1)
        pyautogui.moveTo(1612, 28)
        sleep(1)
        pyautogui.click()

    elif not locateOnScreen('PlaceBet2.PNG', confidence=0.6, region=(550, 365, 200, 50)):
        tentativas += 1

    else:
        num3 = num2
        num2 = num1
        mult0 = mult

        pyautogui.screenshot('screenshot.PNG', region=(85, 1043, 165, 30))
